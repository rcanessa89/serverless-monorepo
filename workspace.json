{
  "version": 2,
  "projects": {
    "utils": {
      "root": "libs/utils",
      "sourceRoot": "libs/utils/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/utils/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/utils"
          ],
          "options": {
            "jestConfig": "libs/utils/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "types": {
      "root": "libs/types",
      "sourceRoot": "libs/types/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/types/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/types"
          ],
          "options": {
            "jestConfig": "libs/types/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "be-api-base": {
      "root": "libs/be-api-base",
      "sourceRoot": "libs/be-api-base/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/be-api-base/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/be-api-base"
          ],
          "options": {
            "jestConfig": "libs/be-api-base/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "be-todos": {
      "root": "apps/be-todos",
      "sourceRoot": "apps/be-todos/src",
      "projectType": "application",
      "prefix": "be-todos",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/be-todos",
            "main": "apps/be-todos/src/main.ts",
            "tsConfig": "apps/be-todos/tsconfig.app.json",
            "assets": [
              "apps/be-todos/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/be-todos/src/environments/environment.ts",
                  "with": "apps/be-todos/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "be-todos:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/be-todos/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/be-todos"
          ],
          "options": {
            "jestConfig": "apps/be-todos/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "be-todos-lib": {
      "root": "libs/be-todos-lib",
      "sourceRoot": "libs/be-todos-lib/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/be-todos-lib/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/be-todos-lib"
          ],
          "options": {
            "jestConfig": "libs/be-todos-lib/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "be-todos"
}